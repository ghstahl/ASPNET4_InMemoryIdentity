@model CustomClientCredentialHost.Models.ConfirmEmailViewModel
@{
    ViewBag.Title = "Email Confirmation Sent";
}

<h2>@ViewBag.Title.</h2>
<div >
    @Html.LabelFor(m => m.Email, new { @class = "col-md-2 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.Email, new { @readonly = "readonly" })
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
