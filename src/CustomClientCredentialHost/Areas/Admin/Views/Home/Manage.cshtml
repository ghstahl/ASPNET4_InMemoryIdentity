@model CustomClientCredentialHost.Areas.Admin.Models.IdentityServerUserModel

@{
    ViewBag.Title = "Manage IdentityServer User";
}

@using (Html.BeginForm("Manage", "Home", FormMethod.Post, new {@class = "form-horizontal", role = "form"}))
{
    @Html.AntiForgeryToken()
    <h4>Manage @ViewBag.Email</h4>
    <hr/>
    @Html.ValidationSummary("", new {@class = "text-danger"})
    <div class="form-group">
        @Html.LabelFor(m => m.UserId, new {@class = "col-md-1 control-label"})
        <div class="col-md-5">
            @Html.TextBoxFor(m => m.UserId, new
            {
                style = "width: 300px;border: none;background-color: #fff;background: transparent;",
                @class = "form-control",
                @readonly = "readonly"
            })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Email, new { @class = "col-md-1 control-label" })
        <div class="col-md-1">
            @Html.TextBoxFor(m => m.Email, new
             {
                 style = "width: 300px;border: none;background-color: #fff;background: transparent;",
                 @class = "form-control",
                 @readonly = "readonly"
             })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Developer, new { @class = "col-md-1 control-label" })
        <div class="col-md-1">
            @Html.CheckBoxFor(m => m.Developer, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Exists, new { @class = "col-md-1 control-label" })
        <div class="col-md-1">
            @Html.CheckBoxFor(m => m.Exists, new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
   
    
    
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Submit"/>
            @Html.ActionLink("Cancel", "Index", "Home", new { @class = "btn btn-default" })
        </div>
    </div>
}


